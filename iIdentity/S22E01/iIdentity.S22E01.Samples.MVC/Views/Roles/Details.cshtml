@model IdentityRole

@{
    ViewData["Title"] = "Role Details";
}

<div class="">
    <h1 class="display-4">User Details</h1>
    <p>Role Information:</p>
</div>

<br />

<div class="form-group">
    <label asp-for="Id"></label>
    <input class="form-control mb-1" type="text" name="Id" value="@Model.Id" readonly />
</div>

<div class="form-group">
    <label asp-for="Name"></label>
    <input class="form-control mb-1" type="text" name="Name" value="@Model.Name" readonly />
</div>

<a class="btn btn-warning btn-lg mb-1" type="submit" asp-controller="Roles" asp-action="Edit" asp-route-Id="@Model.Id">Edit</a>
<a class="btn btn-secondary btn-lg mb-1" asp-controller="Roles" asp-action="Index">Back</a>
